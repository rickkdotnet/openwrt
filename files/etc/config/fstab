config 'global'
	option anon_swap  '0'
	option anon_mount '1'
	option auto_swap  '1'
	option auto_mount '1'
	option delay_root '0'
	option check_fs   '0'

## use 'block detect' to find out uuid, if you want device specific mount.
## anon_mount enables the automatic mounting of /dev/sdXy to /mnt/sdXy

